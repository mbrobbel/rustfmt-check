{
  "name": "rust-rustfmt-check",
  "version": "0.0.0",
  "private": true,
  "description": "Format Rust code using rustfmt",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest -i",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbrobbel/rustfmt-check.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "mbrobbel",
  "license": "MIT",
  "dependencies": {
    "@actions-rs/core": "^0.1.6",
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0",
    "@octokit/rest": "^18.12.0",
    "string-argv": "^0.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^26.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.5.5"
  }
}
