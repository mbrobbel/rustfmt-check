{
  "name": "rust-rustfmt-check",
  "version": "0.0.0",
  "private": true,
  "description": "Format Rust code using rustfmt",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest -i",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbrobbel/rustfmt-check.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "mbrobbel",
  "license": "MIT",
  "dependencies": {
    "@actions-rs/core": "0.0.9",
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.3",
    "@actions/github": "^2.1.1",
    "@octokit/rest": "^17.4.0",
    "string-argv": "^0.3.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/parser": "^2.28.0",
    "@zeit/ncc": "^0.22.1",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^3.4.1",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.5.3",
    "jest-circus": "^25.2.4",
    "js-yaml": "^3.13.1",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "typescript": "^3.6.4"
  }
}
